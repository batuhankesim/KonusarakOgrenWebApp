@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th></th>
            <th>Başlık</th>
            <th>Link</th>
            <th>Açıklama</th>
            <th>Tarihi</th>
        </tr>
    </thead>
    <tbody>
       
            @foreach (var item in ViewBag.RSSFeed)
            {
                <tr>    
                    <td><button type="button" class="btn-btn-sm btn-primary btnSec"> Sınav Oluştur</button> </td>
                    <td>@item.Title</td>
                    <td><a href="@item.Link">@item.Link</a></td>
                    <td>@item.Description</td>
                    <td>@item.PubDate</td>
                </tr>

            }
        
    </tbody>
</table> 